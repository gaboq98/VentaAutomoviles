            @Html.DisplayFor(modelItem => item.IdAutomovil)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdCliente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdTipoPago)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Volver", "Index", "Home",null, new { @class = "btn btn-outline-primary mr-1" })
