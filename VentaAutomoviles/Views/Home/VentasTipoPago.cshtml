@model IEnumerable<VentaAutomoviles.Models.sp_consultar_ventasPorTipoPago1_Result>

@{
    ViewBag.Title = "VentasTipoPago";
}

<h2>Ventas por Tipo de Pago</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdSucursal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdAutomovil)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdCliente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdTipoPago)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
    </tr>

@foreach (var item in Model) {
